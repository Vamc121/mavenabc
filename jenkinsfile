pipeline
{
    agent any
    stages
    {
        stage('ContinuousDownload')
        {
            steps
            {
                git 'https://github.com/Vamc121/maven-web-application.git'
            }
        }
         stage('ContinuousBuild')
        {
            steps
            {
                sh 'mvn package' 
            }
        } 
    }
}
        
